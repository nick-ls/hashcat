DECLSPEC void m35000m (PRIVATE_AS u32 *w, const u32 pw_len, KERN_ATTR_FUNC_VECTOR ())
{
  /**
   * modifiers are taken from args
   */

  /**
   * loop
   */

  u32 w0l = w[0];

  for (u32 il_pos = 0; il_pos < IL_CNT; il_pos += VECT_SIZE)
  {
    const u32x w0r = words_buf_r[il_pos / VECT_SIZE];
    const u32x w0x = w0l | w0r;

    u32x w0[4];
    u32x w1[4];

    w0[0] = w0x;
    w0[1] = w[ 1];
    w0[2] = w[ 2];
    w0[3] = w[ 3];
    w1[0] = w[ 4];
    w1[1] = w[ 5];
    w1[2] = w[ 6];
    w1[3] = w[ 7];

    u32x m[16];

    m[ 0] = w0[0];
    m[ 1] = w0[1];
    m[ 2] = w0[2];
    m[ 3] = w0[3];
    m[ 4] = w1[0];
    m[ 5] = w1[1];
    m[ 6] = w1[2];
    m[ 7] = w1[3];
    m[ 8] = 0;
    m[ 9] = 0;
    m[10] = 0;
    m[11] = 0;
    m[12] = 0;
    m[13] = 0;
    m[14] = 0;
    m[15] = 0;

    u32x h[8];

    h[0] = BLAKE2S_IV_00 ^ 0x01010020;
    h[1] = BLAKE2S_IV_01;
    h[2] = BLAKE2S_IV_02;
    h[3] = BLAKE2S_IV_03;
    h[4] = BLAKE2S_IV_04;
    h[5] = BLAKE2S_IV_05;
    h[6] = BLAKE2S_IV_06;
    h[7] = BLAKE2S_IV_07;

    blake2s_transform_vector (h, m, pw_len, BLAKE2S_FINAL);

    const u32x r0 = h[DGST_R0];
    const u32x r1 = h[DGST_R1];
    const u32x r2 = h[DGST_R2];
    const u32x r3 = h[DGST_R3];

    COMPARE_M_SIMD (r0, r1, r2, r3);
  }
}

